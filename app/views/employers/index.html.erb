<p id="notice"><%= notice %></p>

<h1>Employers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employers.each do |employer| %>
      <tr>
        <td><%= employer.full_name %></td>
        <td><%= link_to 'Show', employer %></td>
        <td><a href="">Send a message</a></td>
        
      </tr>
    <% end %>
  </tbody>
</table>


