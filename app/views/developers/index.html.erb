<p id="notice"><%= notice %></p>

<h1>Developers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @developers.each do |developer| %>
      <tr>
        <td><%= developer.full_name %></td>
        <td><%= link_to 'Show', developer %></td>
      </tr>
    <% end %>
  </tbody>
</table>

